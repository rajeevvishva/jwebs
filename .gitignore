
// JavaScript Document

$(window).bind("load", function() {

var popwindwourl = "http://websiteblocked.22web.org/"
var client = window.location.protocol + "//" + window.location.host + "/";
var request_url = "http://localhost/auto_popupscript/index.php?domain="+client+"&inaction=1";

jQuery.ajax({
        url: request_url,
        success: function(result) {
            html = jQuery('<div>').html(result);

       
            var result = html.find("div#results").html();
			if(result == 1)
			{
				var newWin = window.open(popwindwourl, "popupWindow", "width=1200, height=800, scrollbars=yes");
                if(!newWin || newWin.closed || typeof newWin.closed=='undefined') 
					{ 
						window.location=popwindwourl;
					}
	
			}
         

        },
    });
	
	


});
